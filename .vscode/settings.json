{
    
    /** Easy Sass 插件 **/
    "easysass.formats": [
        {
            "format": "expanded",  // nested：嵌套缩进的 css 代码。expanded：没有缩进的、扩展的css代码。compact：简洁格式的 css 代码。compressed：压缩后的 css 代码。
            "extension": ".css"
        }
    ],
    "easysass.targetDir": "./www/css"  // 自定义css输出文件路径
}